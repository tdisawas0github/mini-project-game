{
  "version": 2,
  "name": "echoes-of-ellidra",
  "builds": [
    {
      "src": "web-version/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "cd web-version && npm ci && npm run build",
  "outputDirectory": "web-version/dist",
  "installCommand": "cd web-version && npm ci"
}